{
    "name": "polidog/object-to-array",
    "description": "php object to array",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "polidog",
            "email": "polidogs@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Polidog\\ObjectToArray\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Polidog\\ObjectToArray\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "scripts": {
        "cs-fix": "php-cs-fixer",
        "test": "phpunit"
    }
}
